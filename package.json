{
  "name": "pnpm-catalog-updates",
  "private": true,
  "description": "A CLI tool to check and update pnpm workspace catalog dependencies",
  "keywords": [
    "pnpm",
    "catalog",
    "dependencies",
    "update",
    "workspace",
    "cli",
    "package-manager"
  ],
  "author": {
    "name": "Evan Hu",
    "email": "evan@yldm.tech"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/houko/pnpm-catalog-updates.git"
  },
  "bugs": {
    "url": "https://github.com/houko/pnpm-catalog-updates/issues"
  },
  "homepage": "https://github.com/houko/pnpm-catalog-updates#readme",
  "engines": {
    "node": ">=22.0.0"
  },
  "packageManager": "pnpm@10.15.1+sha512.34e538c329b5553014ca8e8f4535997f96180a1d0f614339357449935350d924e22f8614682191264ec33d1462ac21561aff97f6bb18065351c162c7e8f6de67",
  "type": "module",
  "scripts": {
    "build": "turbo build",
    "build:watch": "turbo build:watch",
    "dev": "turbo dev --filter=@pcu/cli",
    "start": "pnpm --filter=@pcu/cli start",
    "test": "turbo test",
    "test:watch": "turbo test:watch",
    "test:coverage": "turbo test:coverage",
    "lint": "turbo lint",
    "lint:fix": "turbo lint:fix",
    "format": "turbo format",
    "format:check": "turbo format:check",
    "typecheck": "turbo typecheck",
    "clean": "turbo clean",
    "prepare": "husky install",
    "prepack": "pnpm build",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "node scripts/publish-dual.js"
  },
  "dependencies": {
    "pcu": "workspace:*"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.5",
    "@eslint/js": "catalog:",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.8",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.0.10",
    "@types/semver": "^7.7.0",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "catalog:",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "msw": "^2.10.3",
    "prettier": "catalog:",
    "prettier-plugin-organize-imports": "catalog:",
    "prettier-plugin-tailwindcss": "catalog:",
    "rimraf": "^6.0.1",
    "turbo": "catalog:",
    "tsx": "^4.20.3",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "volta": {
    "node": ">=22",
    "pnpm": ">=10"
  },
  "peerDependencies": {},
  "optionalDependencies": {}
}
