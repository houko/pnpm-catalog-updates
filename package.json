{
  "name": "pnpm-catalog-updates",
  "version": "0.5.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "A CLI tool to check and update pnpm workspace catalog dependencies",
  "keywords": [
    "pnpm",
    "catalog",
    "dependencies",
    "update",
    "workspace",
    "cli",
    "package-manager"
  ],
  "author": {
    "name": "Evan Hu",
    "email": "evan@yldm.tech"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/houko/pnpm-catalog-updates.git"
  },
  "bugs": {
    "url": "https://github.com/houko/pnpm-catalog-updates/issues"
  },
  "homepage": "https://github.com/houko/pnpm-catalog-updates#readme",
  "engines": {
    "node": ">=22.0.0"
  },
  "packageManager": "pnpm@10.12.4+sha512.5ea8b0deed94ed68691c9bad4c955492705c5eeb8a87ef86bc62c74a26b037b08ff9570f108b2e4dbd1dd1a9186fea925e527f141c648e85af45631074680184",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "pcu": "./bin/pcu.js",
    "pnpm-catalog-updates": "./bin/pcu.js"
  },
  "files": [
    "dist",
    "bin",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc && pnpm run build:bin",
    "build:bin": "node scripts/build-bin.js",
    "build:watch": "tsc --watch",
    "dev": "tsx src/cli/index.ts",
    "start": "node dist/cli/index.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:e2e": "vitest --config vitest.e2e.config.ts",
    "lint": "eslint src test --ext .ts,.js",
    "lint:fix": "eslint src test --ext .ts,.js --fix",
    "format": "prettier --write src test",
    "format:check": "prettier --check src test",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist bin/*.js",
    "prepare": "husky install",
    "prepack": "npm run build",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "npm run build && changeset publish"
  },
  "dependencies": {
    "chalk": "catalog:",
    "cli-table3": "catalog:",
    "commander": "catalog:",
    "fs-extra": "catalog:",
    "glob": "catalog:",
    "inquirer": "catalog:",
    "lodash": "catalog:",
    "npm-registry-fetch": "catalog:",
    "ora": "catalog:",
    "pacote": "catalog:",
    "rxjs": "catalog:",
    "semver": "catalog:",
    "yaml": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.5",
    "@eslint/js": "catalog:",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.8",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.0.10",
    "@types/semver": "^7.7.0",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "catalog:",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "msw": "^2.10.3",
    "prettier": "catalog:",
    "rimraf": "^6.0.1",
    "tsx": "^4.20.3",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "lint-staged": {
    "*.{ts,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "volta": {
    "node": ">=22",
    "pnpm": ">=10"
  },
  "peerDependencies": {},
  "optionalDependencies": {}
}
