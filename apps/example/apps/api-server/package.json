{
  "name": "@example/api-server",
  "version": "1.0.0",
  "description": "Example API server using server catalog",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "lint": "eslint src --ext ts",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist .turbo"
  },
  "dependencies": {
    "express": "catalog:server",
    "cors": "catalog:server",
    "helmet": "catalog:server",
    "axios": "catalog:",
    "chalk": "catalog:"
  },
  "devDependencies": {
    "@types/express": "catalog:server",
    "@types/cors": "catalog:",
    "@types/node": "catalog:node20",
    "typescript": "catalog:",
    "vitest": "catalog:",
    "eslint": "catalog:",
    "tsx": "^4.7.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
